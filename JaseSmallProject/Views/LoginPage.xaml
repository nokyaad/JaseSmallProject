<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="JaseSmallProject.Views.LoginPage">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Welcome to This Thing"
                VerticalOptions="CenterAndExpand" 
                HorizontalOptions="CenterAndExpand" />
            <Button x:Name="SigninBtn" Text="Signin" Clicked="SigninBtn_Clicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>